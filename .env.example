# 飞书应用配置
FEISHU_APP_ID=your_app_id_here
FEISHU_APP_SECRET=your_app_secret_here
FEISHU_SPREADSHEET_TOKEN=your_spreadsheet_token_here
FEISHU_SHEET_NAME=your_sheet_name_here
FEISHU_RANGE=A1:Z1000

# 飞书表格列映射（列名或列索引）
FEISHU_PRODUCT_IMAGE_COLUMN=product_image
FEISHU_MODEL_IMAGE_COLUMN=model_image
FEISHU_PROMPT_COLUMN=prompt
FEISHU_STATUS_COLUMN=is_read

# ComfyUI配置
COMFYUI_API_KEY=your_comfyui_api_key_here
COMFYUI_WORKFLOW_ID=your_workflow_id_here
COMFYUI_BASE_URL=https://www.runninghub.cn/task/openapi

# ComfyUI节点ID配置
COMFYUI_PRODUCT_IMAGE_NODE_ID=294
COMFYUI_MODEL_IMAGE_NODE_ID=295
COMFYUI_PROMPT_NODE_ID=prompt_node

# 图生视频工作流配置
COMFYUI_VIDEO_WORKFLOW_ENABLED=true
COMFYUI_VIDEO_WORKFLOW_ID=1959150471611101185
COMFYUI_VIDEO_IMAGE_NODE_ID=293
COMFYUI_VIDEO_PROMPT_NODE_ID=368

# 应用配置
MAX_RETRIES=3
REQUEST_TIMEOUT=30
TEMP_DIR=./temp
OUTPUT_DIR=./output
LOG_LEVEL=INFO
RETRY_DELAY=5
COMFYUI_POLL_INTERVAL=30
COMFYUI_MAX_WAIT_TIME=1800